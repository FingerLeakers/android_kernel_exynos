	usb_host_device_timeout = <0x7ff>; /* Max value */

	usb_audio_offloading = <1>;
	abox = <&abox>;
	/* check susphy support */
	xhci_l2_support = <1>;
};

&usbdrd_phy0 {
	status = "okay";

	/*
	pinctrl-names = "default";
