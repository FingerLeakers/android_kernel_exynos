&s6e3hab {
	size = <70 155>;
};

&panel_drv_0 {
    status = "okay";
	ddi_info = <&s6e3ha9>;

	#address-cells = <2>;
	#size-cells = <1>;
	ranges;
	pinctrl-names = "default";
	pinctrl-0 = <&disp_det_ctrl &disp_te_ctrl>;

	gpios {
		disp-reset {
			name = "disp-reset";
			gpios = <&gpa3 4 1>;	/* ACTIVE_LOW */
			dir = <0>;  /* OUT */
			irq-type = <0x0>;   /* NONE */
		};

		disp-det {
			name = "disp-det";
			gpios = <&gpm17 0 1>;  /* 0:OK, 1:NOK(active) */
			dir = <1>;  /* IN */
			irq-type = <0x2>;   /* FALLING EDGE */
			irq-pend {
				reg = <0x0 0x15850A34 0x0>;
				bit = <0x2>;
			};
		};

		err-fg {
			name = "err-fg";
			gpios = <&gpa3 5 0>; 		/* 0:NOK(active) 1:OK */
			dir = <1>;  				/* IN */
			irq-type = <0x00002001>;	/* RISING EDGE | ONE_SHOT */
		};
	};

	regulators {
		ddi-vci {
			regulator = <&dp_ldo4>;
			def-voltage = <3000000>;
			lpm-voltage = <3000000>;
		};

		ddi-vdd3 {
			regulator = <&dp_ldo1>;
			def-voltage = <1800000>;
			lpm-voltage = <1800000>;
		};

		ddr-vddr {
			regulator = <&dp_buck>;
			def-voltage = <1800000>;
			lpm-voltage = <1500000>;
		};

		short-detect {
			regulator = <&dp_elvss>;
			def-current = <2000>;
			lpm-current = <8000>;
		};
	};
	panel-lookup {
		panel-name =
			"s6e3ha9_davinci2_a3_s0_default",
			"s6e3hab_hubble1_a3_s0_default",
			"s6e3hab_hubble2_a3_s0_default",
			"s6e3hab_hubble3_a3_s0_default",
			"s6e3hab_hubble1_a3_s0_preliminary",
			"s6e3hab_hubble2_a3_s0_preliminary",
			"s6e3hab_hubble3_a3_s0_preliminary",
			"s6e3hab_hubble1_a3_s0_preliminary_no_osc",
			"s6e3hab_hubble2_a3_s0_preliminary_no_osc",
			"s6e3hab_hubble3_a3_s0_preliminary_no_osc",
			"s6e3hab_hubble1_a3_s0_default_no_osc",
			"s6e3hab_hubble2_a3_s0_default_no_osc",
			"s6e3hab_hubble3_a3_s0_default_no_osc";
		panel-ddi-info = <&s6e3ha9 &s6e3hab>;
		panel-lut = <
			0xa10013 0xff00ff 0x0 0x0  /* S6E3HA9 1440x3040 PANEL*/

			0x000000 0x0030C3 0x7 0x1  /* S6E3HAB 1440x3200 X1 PANEL */
			0x000001 0x0030C3 0x7 0x1  /* S6E3HAB 1440x3200 X1 PANEL */
			0x000000 0x0030C0 0xA 0x1  /* S6E3HAB 1440x3200 X1 PANEL */
			0x000040 0x0030C3 0x4 0x1  /* S6E3HAB 1440x3200 X1 PANEL */
			0x000041 0x0030C3 0x4 0x1  /* S6E3HAB 1440x3200 X1 PANEL */
			0x000000 0x003000 0x1 0x1  /* S6E3HAB 1440x3200 X1 PANEL */

			0x001000 0x0030C3 0x8 0x1  /* S6E3HAB 1440x3200 Y2 PANEL */
			0x001001 0x0030C3 0x8 0x1  /* S6E3HAB 1440x3200 Y2 PANEL */
			0x001000 0x0030C0 0xB 0x1  /* S6E3HAB 1440x3200 Y2 PANEL */
			0x001040 0x0030C3 0x5 0x1  /* S6E3HAB 1440x3200 Y2 PANEL */
			0x001041 0x0030C3 0x5 0x1  /* S6E3HAB 1440x3200 Y2 PANEL */
			0x001000 0x003000 0x2 0x1  /* S6E3HAB 1440x3200 Y2 PANEL */

			0x002000 0x0030C3 0x9 0x1  /* S6E3HAB 1440x3200 Z3 PANEL */
			0x002001 0x0030C3 0x9 0x1  /* S6E3HAB 1440x3200 Z3 PANEL */
			0x002000 0x0030C0 0xC 0x1  /* S6E3HAB 1440x3200 Z3 PANEL */
			0x002040 0x0030C3 0x6 0x1  /* S6E3HAB 1440x3200 Z3 PANEL */
			0x002041 0x0030C3 0x6 0x1  /* S6E3HAB 1440x3200 Z3 PANEL */
			0x002000 0x003000 0x3 0x1  /* S6E3HAB 1440x3200 Z3 PANEL */

			0x000000 0x000000 0x2 0x1  /* default */
		>;
	};
};

