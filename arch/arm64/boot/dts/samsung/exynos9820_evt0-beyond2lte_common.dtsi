	#size-cells = <0>;

	sx9330-i2c@28 {
		compatible = "sx9330";
		reg = <0x28>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&grip_irq>;
		interrupt-parent = <&gpa3>;
		interrupts = <0 0 0>;

		sx9330,nirq-gpio = <&gpa3 0 0>;

