				compatible = "samsung,usb-notifier";
				/*
				 * USB_LDOCONTROL_EXYNOS8895	1
				 * USB_LDOCONTROL_EXYNOS9810	2
				 * USB_LDOCONTROL_EXYNOS9820	3
