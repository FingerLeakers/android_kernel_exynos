		sx9330,adcfiltph1_reg = <0x00227F10>;    /* Threshold : 8064.5, ADCFILTSAMPLES : 1 */
		sx9330,avgbfilt_reg = <0x00000C80>;      /* AVGPOSFILT : 1/256, AVGNEGFILT : 1/2 */
	};
};

